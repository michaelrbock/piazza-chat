{
  "name": "Download Selected Links",
  "description": "Select links on a page and download them.",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["https://piazza.com/*"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["jquery.js", "chat.js", "jquery.min.js", "socket.io-1.0.6.js"],
      "matches": ["https://piazza.com/*"]
    }
  ],
  "background": {
    "scripts": ["jquery.js", "background.js"]
  }
}
